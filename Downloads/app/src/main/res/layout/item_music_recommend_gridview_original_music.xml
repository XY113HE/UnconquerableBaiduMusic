<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="5dp">
    <ImageView
        android:id="@+id/iv_item_music_recommend_gridview_original_music_bg"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:scaleType="fitXY"/>
    <TextView
        android:layout_marginTop="3dp"
        android:id="@+id/tv_item_music_recommend_gridview_original_music_title"
        android:layout_below="@id/iv_item_music_recommend_gridview_original_music_bg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="12dp"
        android:textColor="#000"
        android:text="T榜第一期年榜预选"/>
</RelativeLayout>